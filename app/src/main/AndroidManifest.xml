<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.assignment">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/e_stationary_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/e_stationary_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

        <activity android:name=".Register"/>
        <activity android:name=".AccountInfo"/>
        <activity android:name=".AddToCart"/>
        <activity android:name=".Complain"/>
        <activity android:name=".CustomerService"/>
        <activity android:name=".DeliveryDetails"/>
        <activity android:name=".Home"/>
        <activity android:name=".Done"/>
        <activity android:name=".ItemDetails"/>
        <activity android:name=".Payment"/>
        <activity android:name=".Settings"/>
        <activity android:name=".TalkToUs"/>
    </application>
</manifest>